using System.Windows.Forms;

namespace SlotMachine
    {
    public partial class Play_Table : Form
        {
        public Play_Table()
            {
            InitializeComponent();
            }

        private void btnReturn_Click(object sender, EventArgs e)
            {
            frmSlotMachine slotmachine = new frmSlotMachine();
            this.Hide();
            slotmachine.Show();
            }
        }
    }
